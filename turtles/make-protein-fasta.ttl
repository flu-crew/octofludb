PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX f: <https://github.com/arendsee/flucrew/term/>

SELECT ?gb ?pid ?s ?segment ?seq
WHERE {
    ?strain a f:strain_id .
    ?strain f:tag 'swine-reference' .
    ?strain rdfs:label ?s .
    ?strain f:has_segment ?gb .
    ?gb f:feature ?fid .
    ?fid a f:CDS .
    ?fid a f:feature .
    ?fid f:gene ?segment .
    ?fid f:protein_id ?pid .
    ?fid f:translation ?seq .
}
LIMIT 10
