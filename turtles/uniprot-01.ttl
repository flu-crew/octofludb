PREFIX taxon:<http://purl.uniprot.org/taxonomy/>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX core: <http://purl.uniprot.org/core/> 
PREFIX embl: <http://purl.uniprot.org/embl/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX dc: <http://purl.org/dc/elements/1.1/> 
PREFIX uniprot: <http://purl.uniprot.org/uniprot/>
SELECT DISTINCT ?s ?p ?o
WHERE
{
    ?taxon a core:Taxon .
    ?taxon core:scientificName "Influenza A virus (A/Malaysia/1715991/2007(H1N1))" .
    ?taxon rdfs:subClassOf taxon:11320 .
    ?s core:organism ?taxon .
    ?s core:sequence ?sid .
    ?sid core:modified ?modified .
    ?sid core:crc64Checksum ?checksum .
    ?sid rdf:value ?sequence .
}
