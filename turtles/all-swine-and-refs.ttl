SELECT DISTINCT ?gb
WHERE {
    {
        ?event a usda:event .
        ?event foaf:name "glyco-project" .
        ?event usda:related_to ?gb_id .
        ?gb_id foaf:name ?gb .
    } UNION {
        ?strain_id usda:host "Swine" .
        ?strain_id a usda:strain_id .
        ?strain_id usda:has_segment ?gb_id .
        ?gb_id foaf:name ?gb .
    }
}
