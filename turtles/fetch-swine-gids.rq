PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX f: <https://flu-crew.org/term/>
PREFIX onto: <http://www.ontotext.com/>

SELECT DISTINCT ?gb
FROM onto:disable-sameAs
WHERE {
    {
        ?sid f:host "swine" .
        ?sid f:has_segment ?gid .
        ?gid f:genbank_id ?gb .
    } UNION {
        ?sid f:tag/f:name ?tag .
        ?sid f:has_segment ?gid .
        ?gid f:genbank_id ?gb .
    }
}
