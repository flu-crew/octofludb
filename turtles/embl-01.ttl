# example from the EMBL website
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX dbpedia2: <http://dbpedia.org/property/>
PREFIX dbpedia: <http://dbpedia.org/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX faldo: <http://biohackathon.org/resource/faldo#>
PREFIX ensembl: <http://rdf.ebi.ac.uk/resource/ensembl/>
PREFIX biopax: <http://www.biopax.org/release/biopax-level3.owl#>

SELECT DISTINCT ?name ?xref_db ?xref {
    # ?s ?fromTaxon <http://identifiers.org/taxonomy/11320> .
    # ?s a <http://rdf.ebi.ac.uk/terms/chembl#UniprotRef> .
    # ?e ?hasUniprot ?s .
    # ?e a ?o .
    ?e a biopax:Protein .
    ?e biopax:name ?name .
    ?e biopax:xref ?xref_id .
    ?xref_id biopax:id ?xref .
    ?xref_id biopax:db ?xref_db .
}
LIMIT 30
