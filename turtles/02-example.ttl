SELECT ?event ?i ?id ?strain_name ?year
WHERE {
    ?event a usda:event .
    ?event usda:related_to ?i .
    ?i foaf:name ?id .
    ?i usda:date ?date .
    OPTIONAL{
        ?i usda:xref ?strain_id .
        ?strain_id foaf:name ?strain_name .
    }
    BIND (YEAR(?date) AS ?year) .
    FILTER (?year = 2017) .
}
