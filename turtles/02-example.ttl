PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX f: <https://github.com/arendsee/flucrew/term/>

SELECT ?id ?strain ?year
WHERE {
    ?i f:tag "fair" .
    ?i rdfs:label ?id .
    ?i f:date ?date .
    BIND (YEAR(?date) AS ?year) .
    FILTER (?year = 2017) .
    # OPTIONAL{
    #     ?strain_id f:xref ?i  .
    #     ?strain_id a f:strain_id .
    #     ?strain_id rdfs:label ?strain .
    #     ?strain_id f:xref ?j .
    #     ?j a f:a0_id .
    #     ?j rdfs:label ?a0 .
    # }
}
