SELECT ?id ?strain ?gb ?year
WHERE {
    ?event a usda:event .
    ?event usda:related_to ?i .
    ?i foaf:name ?id .
    ?i usda:date ?date .
    ?db_id foaf:name ?gb .
    OPTIONAL{
        ?strain usda:xref ?i .
        ?strain a usda:strain_id .
    }
    BIND (YEAR(?date) AS ?year) .
    FILTER (?year = 2017) .
}
