PREFIX f: <https://github.com/arendsee/flucrew/term/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

SELECT ?id ?strain ?year
WHERE {
    ?i f:tag "fair" .
    ?i foaf:name ?id .
    ?i f:date ?date .
    BIND (YEAR(?date) AS ?year) .
    FILTER (?year = 2017) .
