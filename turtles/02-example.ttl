PREFIX usda: <https://github.com/arendsee/flucrew/term/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

SELECT ?id ?strain ?year
WHERE {
    ?event a usda:event .
    ?event foaf:name "fair" .
    ?event usda:related_to ?i .
    ?i foaf:name ?id .
    ?i usda:date ?date .
    BIND (YEAR(?date) AS ?year) .
    FILTER (?year = 2017) .
    # OPTIONAL{
    #     ?strain_id usda:xref ?i  .
    #     ?strain_id a usda:strain_id .
    #     ?strain_id foaf:name ?strain .
    #     ?strain_id usda:xref ?j .
    #     ?j a usda:a0_id .
    #     ?j foaf:name ?a0 .
    # }
}
