PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX f: <https://github.com/arendsee/flucrew/term/>

SELECT ?ref_reason ?gb ?strain_name ?subtype ?segment ?ha_clade ?date ?seq
WHERE {
    ?strain a f:strain_id .
    ?strain rdfs:label ?strain_name .
    ?strain f:ref_reason ?ref_reason .
    ?strain f:subtype ?subtype .
    ?strain f:ha_clade ?ha_clade .
    ?strain f:date ?date .
    ?strain f:has_segment ?gb .
    ?gb f:feature ?fid .
    ?gb f:GBSeq_sequence ?seq .
    ?fid a f:CDS .
    ?fid a f:feature .
    ?fid f:gene ?segment .
}
